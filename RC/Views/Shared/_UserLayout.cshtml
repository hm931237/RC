<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">
    
    @Styles.Render("~/admin-lte/css")
    <link href="~/Content/navbar/navbar.css" rel="stylesheet" />


    <style>
        .img-responsive{
            height:99px;
        }
    </style>
    <style>
        #feedback {
        z-index: 10;
    transform: rotate(90deg);
    float: right;
    position: fixed;
    top: calc(97% - 47px);
    left: 80px;
        }

            #feedback a {
                background: #34c6d3;
                border-radius: 5px 0 0 5px;
                box-shadow: 0 0 3px rgba(0, 0, 0, .3);
                border: 3px solid #fff;
                border-right: 0;
                display: block;
                padding: 20px 12px;
                transition: all .2s ease-in-out;
            }

                #feedback a:hover {
                    padding-right: 20px;
                }
    </style>

</head>
<body class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper">
        @Html.Partial("_NavBarPartial")
        @Html.Partial("_SideBarPartial")
        <div class="content-wrapper">
            <section class="content-header">
                <br />
                <br />
            </section>
            <section class="content">
                <div class="row">
                    <div class="col-md-3 col-md-push-9 col-sm-6 col-sm-push-6 col-xs-12">
                        @Html.Partial("_userStaticSections")
                    </div>
                    <div class="col-md-9 col-md-pull-3 col-sm-6 col-sm-pull-6 col-xs-12">
                        @RenderBody()
                    </div>
                </div>
            </section>
        </div>
        @Html.Partial("_Footer")
        <div class="control-sidebar-bg"></div>
    </div>

    <div class="modal fade" id="modalContactForm" tabindex="-1" role="dialog" aria-labelledby="contactLabel" aria-hidden="true">
        <div class="modal-dialog">
            <div class="panel panel-primary">
                <div class="panel-heading">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                    <h4 class="panel-title" id="contactLabel"><span class="glyphicon glyphicon-info-sign"></span> Any questions? Feel free to contact us.</h4>
                </div>
                <form action="#" id="feedbackForm" accept-charset="utf-8">
                    <div class="modal-body" style="padding: 5px;">
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12" style="padding-bottom: 10px;">
                                <input id="feedbackFormFullName" required class="form-control" style="display: block;width: 100%;height: 34px;font-size: 14px;line-height: 1.428571429;color: #555;background-color: #fff;background-image: none;border: 1px solid #ccc;border-radius: 4px;" name="email" placeholder="FullName" type="text" />
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12" style="padding-bottom: 10px;">
                                <input id="feedbackFormE-mail" required class="form-control" style="display: block;width: 100%;height: 34px;font-size: 14px;line-height: 1.428571429;color: #555;background-color: #fff;background-image: none;border: 1px solid #ccc;border-radius: 4px;" name="email" placeholder="E-mail" type="text" />
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <textarea id="feedbackFormComment" style="resize:vertical;display: block;width: 100%;height: 34px;font-size: 14px;line-height: 1.428571429;color: #555;background-color: #fff;background-image: none;border: 1px solid #ccc;border-radius: 4px;" class="form-control" placeholder="Message..." rows="6" name="comment"></textarea>
                            </div>
                        </div>
                    </div>
                    <div class="panel-footer" style="margin-bottom:-14px;">
                        <input type="button" class="btn btn-success" value="Send" onclick="Feedback(@Convert.ToInt32(Session["LoginUserTypeId"]));" />
                        <!--<span class="glyphicon glyphicon-ok"></span>-->
                        <input type="reset" class="btn btn-danger" value="Clear" />
                        <!--<span class="glyphicon glyphicon-remove"></span>-->
                        <button style="float: right;" type="button" class="btn btn-default btn-close" data-dismiss="modal">Close</button>
                    </div>
            </div>
        </div>
    </div>
    </div>
    <div id="feedback">
        <a href="#" type="button" class="btn btn-info btn-lg" data-toggle="modal" data-target="#modalContactForm">
            <img src="data:image/svg+xml;base64,"
                 alt="Feedback" title="Feedback Button" height="70px" />
        </a>
    </div>
