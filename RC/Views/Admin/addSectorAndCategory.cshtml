    @section Scripts {
        @Scripts.Render("~/bundles/jqueryval")
        <script src='@Url.Content("~/Scripts/jquery.validate.js")' type='text/javascript'></script>
        <script src='@Url.Content("~/Scripts/jquery.validate.unobtrusive.js")' type='text/javascript'></script>

        <script>
            $(document).ready(function () {
                $('#Type').on('change', function () {
                    if (this.value == '1') {
                        $("#category").hide();
                        $("#factor").hide();
                        $("#sector").show();

                    }
                    if (this.value == '2') {

                        $("#sector").hide();
                        $("#factor").hide();
                        $("#category").show();
                    }
                    if (this.value == '3') {

                        $("#sector").hide();
                        $("#category").hide();
                        $("#factor").show();
                    }
                });

                var glyphIcon = "";
                $(".glyph").click(function () {

                    glyphIcon = $(this).children(".glyphicon-class").text();
                    $(".box_Icon").fadeOut();


                });



                $("#saveSector").click(function () {

                    if ($("#sectorName").val() != "" && glyphIcon == "") {
                        $(".box_Icon").fadeIn();
                        toastr.success('Please Choose Icon');
                    }

                    else if ($('#sectorName').val() == "") {
                        toastr.error('Please Choose Sector Name');
                    }
                    else if (glyphIcon == "") {
                        toastr.error('Please Choose Icon for your Sector');

                    }
                    else {
                        var Sector = new Object();
                        Sector.name = $('#sectorName').val();
                        Sector.iconPath = glyphIcon;


                        $.ajax({
                            url: '/api/Sector/Sector',
                            data: Sector,
                            method: "POST",
                            success: function () {

                                toastr.success("Sector Added Successuffly...");
                                glyphIcon = "";
                                $("#sectorName").val("");

                            },
                            error: function (err) {
                                toastr.error('Failed To Add Sector!');

                            }

                        });
                    }
                });


                $("#saveCategory").click(function () {
                    var Category = new Object();
                    Category.name = $('#categoryName').val();
                    Category.sectorId = $('#sectorId').val();
                    $.ajax({
                        url: '/api/Category/Category',
                        data: Category,
                        method: "POST",
                        success: function () {
                            toastr.success('Category Added Successuffly');

                        },
                        error: function () {
                            toastr.error('Failed To Add a Category!');
                        }
                    });
                });

                $("#saveFactor").click(function () {
                    var Factor = new Object();
                    Factor.name = $('#factorName').val();
                    Factor.categoryId = $('#categoryId').val();
                    $.ajax({
                        url: '/api/Factor/Factor',
                        data: Factor,
                        method: "POST",
                        success: function () {
                            toastr.success('Factor Added Successuffly');

                        },
                        error: function () {
                            toastr.error('Failed To Add Factor!');
                        }
                    });
                });
            });

        </script>
